{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "project": [
        "tsconfig.json"
    ],
    "entry": [
        "src/app/**/*.{ts,tsx}",
        "src/features/**/*.{ts,tsx}",
        "src/lib/**/*.ts",
        "src/types/**/*.ts"
    ],
    "ignore": [
